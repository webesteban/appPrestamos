<%= form_with model: expense_type, data: { turbo: false } do |f| %>
  <div class="mb-3">
    <%= f.label :name, class: "form-label" %>
    <%= f.text_field :name, class: "form-control" %>
  </div>
  <div class="mb-3">
    <%= f.label :max_value, class: "form-label" %>
    <%= f.number_field :max_value, class: "form-control" %>
  </div>
  <div class="d-flex justify-content-end">
    <%= f.submit class: "btn btn-primary" %>
  </div>
<% end %>
