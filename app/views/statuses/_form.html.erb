<%= form_with model: status, data: { turbo: false } do |f| %>
  <div class="mb-3">
    <%= f.label "NOMBRE", class: "form-label text-uppercase" %>
    <%= f.text_field :name, class: "form-control" %>
  </div>
  <div class="d-flex justify-content-end">
    <%= f.submit "GUARDAR", class: "btn btn-primary" %>
  </div>
<% end %>
